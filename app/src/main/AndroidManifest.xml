<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.xin.dormitory">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.xin.dormitory.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.xin.dormitory.RegisterActivity" />
        <activity android:name="com.example.xin.dormitory.LoginHActivity" />
        <activity android:name="com.example.xin.dormitory.LoginSActivity"
            android:label="宿舍管理"/>
        <activity
            android:name="com.example.xin.dormitory.dormitoryActivity"
            android:label="宿舍天地" />
        <activity android:name="com.example.xin.dormitory.MyDormitory"
            android:label="事务管理"/>
    </application>

</manifest>